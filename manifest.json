{
    "name": "Bookmark Shortcuts",
    "version": "1.0",
    "manifest_version": 2,
    "background": { "scripts": ["background.js"] },
    "content_scripts": [{
        "js": [ "content.js" ],
        "matches": [ "http://*/*", "https://*/*" ]
    }],
   "description": "Keyboard shortcuts for your bookmark bar",
   "permissions": ["bookmarks", "tabs", "http://*/*", "https://*/*"]
}
